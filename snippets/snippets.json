    "p5.js Polygon": {
        "prefix": "p5-polygon",
        "body": [
            "function polygon(x, y, radius, sides = 3, angle = 0) {",
            "   beginShape();",
            "   for (let i = 0; i < sides; i++) {",
            "       const a = angle + TWO_PI * (i / sides);",
            "       let sx = x + cos(a) * radius;",
            "       let sy = y + sin(a) * radius;",
            "       vertex(sx, sy);",
            "   };",
            "   endShape(CLOSE);",
            "};"
        ],
        "description": "Function to draw polygons in p5.js"
    },
    "p5.js Segment": {
        "prefix": "p5-segment",
        "body": [
            "function segment(x, y, length, angle = 0) {",
            "   const r = length / 2;",
            "   const u = Math.cos(angle);",
            "   const v = Math.sin(angle);",
            "   line(x - u * r, y - v * r, x + u * r, y + v * r);",
            "};"
        ],
        "description": "Function to draw line segments in p5.js"
    },
    "p5.js Draw Grid": {
        "prefix": "p5-grid",
        "body": [
            "function drawGrid(sizeX, sizeY) {",
            "   stroke(200);",
            "   fill(120);",
            "   const spaceX = width/sizeX",
            "   const spaceY = height/sizeY",
            "   for (let x=0; x < width; x+=spaceX) {",
            "       line(x, 0, x, height);",
            "   };",
            "   for (let y=0; y < width; y+=sizeY) {",
            "       line(0, y, width, y);",
            "   };",
            "};"
        ]
    }
}